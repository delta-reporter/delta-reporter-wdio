{
  "name": "wdio-delta-reporter-service",
  "version": "0.0.1",
  "description": "Delta Reporter service for WebdriverIO",
  "main": "index.js",
  "files": [
    "index.js",
    "compare.js",
    "launcher.js",
    "*.md",
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib .tmp",
    "build": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/tsc && ./node_modules/.bin/wdio ./test/wdio/wdio.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/tsc --pretty && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json}": [
      "prettier --write --print-width 120",
      "git add -f"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/delta-reporter/wdio-delta-reporter-service"
  },
  "bugs": {
    "url": "https://github.com/delta-reporter/wdio-delta-reporter-service/issues"
  },
  "author": "Juan Negrier <juannegrier@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.5",
    "@wdio/cli": "^5.16.9",
    "@wdio/dot-reporter": "^5.18.6",
    "@wdio/local-runner": "^5.16.9",
    "@wdio/mocha-framework": "^5.16.5",
    "@wdio/selenium-standalone-service": "^5.16.5",
    "@wdio/spec-reporter": "^5.18.7",
    "@wdio/sync": "^5.16.9",
    "chai": "^4.2.0",
    "lint-staged": "^8.1.7",
    "mocha": "^5.2.0",
    "prettier": "^1.17.1",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.16.0",
    "typescript": "^3.7.4",
    "webdriverio": "^5.18.4"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "axios-retry": "^3.1.2",
    "husky": "^3.0.8",
    "wdio-mocha-framework": "^0.6.4"
  }
}
