{
  "name": "@delta-reporter/wdio-delta-reporter-service",
  "version": "0.0.2",
  "description": "Delta Reporter service for WebdriverIO",
  "main": "index.js",
  "files": [
    "index.js",
    "compare.js",
    "launcher.js",
    "*.md",
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib .tmp",
    "build": "tsc",
    "test": "./node_modules/.bin/tsc && ./node_modules/.bin/wdio ./test/wdio/wdio.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/tsc --pretty && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json}": [
      "prettier --write --print-width 120",
      "git add -f"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delta-reporter/wdio-delta-reporter-service.git"
  },
  "bugs": {
    "url": "https://github.com/delta-reporter/wdio-delta-reporter-service/issues"
  },
  "author": "Juan Negrier <juannegrier@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.21",
    "@wdio/cli": "^5.22.4",
    "@wdio/dot-reporter": "^5.22.4",
    "@wdio/local-runner": "^5.22.4",
    "@wdio/mocha-framework": "^5.16.5",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.22.4",
    "@wdio/sync": "^5.16.9",
    "chai": "^4.2.0",
    "gm": "^1.23.1",
    "image-size": "^0.8.3",
    "jimp": "^0.12.1",
    "lint-staged": "^8.1.7",
    "mocha": "^5.2.0",
    "prettier": "^1.17.1",
    "rimraf": "^3.0.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.16.0",
    "typescript": "^3.7.4",
    "wdio-docker-service": "^2.3.1",
    "wdio-novus-visual-regression-service": "^0.1.1",
    "webdriverio": "^5.22.4"
  },
  "dependencies": {
    "argos-cli": "^0.1.3",
    "axios": "^0.19.2",
    "axios-retry": "^3.1.8",
    "husky": "^3.0.8",
    "wdio-mocha-framework": "^0.6.4"
  },
  "homepage": "https://github.com/delta-reporter/wdio-delta-reporter-service#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "reporter",
    "delta-reporter"
  ]
}
